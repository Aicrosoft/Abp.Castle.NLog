<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Description>Abp的NLog日志输出模块。</Description>
    <Copyright>Copyright © risesoho.com</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>RiseSoho,RiseSoft,Abp,NLog,Log</PackageTags>
    <RootNamespace>Abp</RootNamespace>
    <PackageLicenseUrl>https://github.com/RiseSoho/Abp.Castle.NLog/blob/dev/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/RiseSoho/Abp.Castle.NLog</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
